{
  "name": "cypress-babel-esx-preprocessor",
  "version": "1.0.0",
  "description": "Cypress plugin to allow you to use all proposals to the JavaScript language at Proposal stage or above by using babel-preset-stage-1 as well as all JavaScript that has been finalised in the ECMA standard by using babel-preset-env",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js index.spec.js",
    "test": "mocha --reporter spec \"test/index.spec.js\"",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chinchiheather/cypress-babel-esx-preprocessor.git"
  },
  "keywords": [
    "cypress",
    "cypress-plugin",
    "cypress-preprocessor",
    "es2015",
    "es2016",
    "es2017",
    "babel",
    "babel-preset-stage-1",
    "babel-preset-env"
  ],
  "author": {
    "name": "Heather Roberts",
    "email": "heather.roberts427@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@cypress/browserify-preprocessor": "^1.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "resolve": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "mocha": "^5.0.5",
    "mockery": "^2.1.0",
    "sinon": "^4.5.0"
  }
}
